{
  "version": "0.0.0",
  "name": "@nerimity/solid-emoji-picker",
  "description": "SolidJS Emoji Picker modal.",
  "license": "MIT",
  "author": "Supertigerr",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nerimity/solid-emoji-picker.git"
  },
  "homepage": "https://github.com/Nerimity/solid-emoji-picker#readme",
  "bugs": {
    "url": "https://github.com/Nerimity/solid-emoji-picker/issues"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      },
      "require": "./dist/dev.cjs"
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "typesVersions": {},
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup src/index.tsx src/categories.ts src/emojis.ts",
    "test": "concurrently pnpm:test:*",
    "test:client": "vitest",
    "test:ssr": "pnpm run test:client --mode ssr",
    "prepublishOnly": "pnpm build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "update-deps": "pnpm up -Li",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "solid-js": "^1.6.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "esbuild": "^0.17.8",
    "esbuild-plugin-solid": "^0.5.0",
    "jsdom": "^21.1.0",
    "prettier": "2.8.4",
    "solid-js": "^1.6.11",
    "tsup": "^6.6.3",
    "tsup-preset-solid": "^0.1.5",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-solid": "^2.5.0",
    "vitest": "^0.28.5"
  },
  "keywords": [
    "solid"
  ],
  "packageManager": "pnpm@7.22.0",
  "dependencies": {
    "@minht11/solid-virtual-container": "^0.2.1",
    "solid-styled-components": "^0.28.5"
  },
  "directories": {
    "test": "test"
  }
}
